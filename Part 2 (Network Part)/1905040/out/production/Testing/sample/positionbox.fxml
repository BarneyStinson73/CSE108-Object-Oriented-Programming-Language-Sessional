<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Positionbox">
   <children>
      <AnchorPane layoutX="110.0" layoutY="81.0" prefHeight="219.0" prefWidth="388.0">
         <children>
            <Text layoutX="136.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Position Based Search" underline="true">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Text>
            <Text fill="#e11616" layoutX="25.0" layoutY="114.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Desired Position" />
            <TextField fx:id="playerpos" layoutX="129.0" layoutY="97.0" prefHeight="25.0" prefWidth="189.0" promptText="gk/midfielder/defender/forward" />
            <Button fx:id="searchbyP" layoutX="211.0" layoutY="161.0" mnemonicParsing="false" onAction="#searchByPosition" text="Search" />
            <Button fx:id="previousfromposition" layoutX="53.0" layoutY="161.0" mnemonicParsing="false" onAction="#backFromPosition" text="Back to Previous" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
