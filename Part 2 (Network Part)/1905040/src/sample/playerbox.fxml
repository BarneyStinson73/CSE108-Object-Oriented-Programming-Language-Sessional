<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.playerBoxController">
   <children>
      <AnchorPane layoutX="85.0" layoutY="57.0" prefHeight="248.0" prefWidth="464.0">
         <children>
            <Text fill="#e40707" layoutX="152.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search by Player Name">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Text>
            <TextField fx:id="playernametext" layoutX="232.0" layoutY="112.0" />
            <Text layoutX="62.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Input the player name here" />
            <Button fx:id="search" layoutX="292.0" layoutY="166.0" mnemonicParsing="false" onAction="#searchByPlayerName" text="Search" />
            <Button fx:id="previousfromsearchbyplayer" layoutX="118.0" layoutY="166.0" mnemonicParsing="false" onAction="#previousFromSearchByP" text="Back to previous" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
